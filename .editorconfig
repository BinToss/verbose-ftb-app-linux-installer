# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.sh]
#indent_style = space
#indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true
[*.gz]
indent_style = unset
indent_size = unset
end_of_line = unset
charset = unset
trim_trailing_whitespace = false
insert_final_newline = false
tab_width = unset
